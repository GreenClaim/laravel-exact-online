{
    "name": "greenclaim/laravel-exact-online",
    "description": "Laravel Facade wrapper for Exact Online PHP API",
    "require": {
        "picqer/exact-php-client": "^3.1"
    },
    "require-dev": {
      "phpunit/phpunit": "~5.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Rik Morskate",
            "email": "technical@yource.com",
            "homepage": "https://dev.yource.com",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Yource\\LaravelExactOnline\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yource\\LaravelExactOnline\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Yource\\LaravelExactOnline\\Providers\\LaravelExactOnlineServiceProvider"
            ],
            "aliases": {
                "ExactOnline": "Yource\\LaravelExactOnline\\LaravelExactOnlineFacade"
            }
        }
    }
}
